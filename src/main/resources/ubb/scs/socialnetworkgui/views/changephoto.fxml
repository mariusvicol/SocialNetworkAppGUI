<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.image.ImageView?>

<?import javafx.scene.control.Button?>

<VBox spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="ubb.scs.socialnetworkgui.gui.ChangePhotoController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <VBox alignment="CENTER">
        <ImageView fx:id="imageView" fitWidth="400" fitHeight="400" styleClass="image-view"/>
        <Button text="Browse for Image" onAction="#browseImage"/>
    </VBox>
</VBox>
