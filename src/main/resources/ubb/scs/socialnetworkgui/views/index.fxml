<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="ubb.scs.socialnetworkgui.gui.StartPageController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <Label fx:id="welcomeText" text="SOCIAL NETWORK" styleClass="label_title"/>
  <HBox alignment="CENTER" spacing="10">
    <Button fx:id="login" text="Login" onAction="#onLoginClick"/>
    <Button fx:id="signUp" text="Sign Up" onAction="#onSignUpClick"/>
  </HBox>

</VBox>
